{
  "name": "react_webpack_starter",
  "version": "1.0.0",
  "description": "Boilerplate for React apps",
  "main": "index.js",
  "scripts": {
    "start": "concurrently \"webpack --mode production\" \"cross-env NODE_ENV=dev nodemon --exec babel-node server/index.js\"",
    "server": "cross-env NODE_ENV=dev nodemon --exec babel-node build/server.js",
    "dev": "webpack-dev-server --mode development --inline --open --hot",
    "build": "webpack --mode production"
  },
  "author": "Play Ra",
  "license": "MIT",
  "dependencies": {
    "cross-env": "^5.1.4",
    "react": "^16.3.2",
    "react-art": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-native-web": "^0.6.1",
    "react-router-dom": "^4.2.2",
    "url-loader": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-react-native-web": "^0.6.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.1.0",
    "image-webpack-loader": "^4.2.0",
    "react-native-web-image-loader": "^0.0.5",
    "style-loader": "^0.20.3",
    "webpack": "^4.4.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1"
  }
}
